#!groovy
// Check remote host properties
properties([disableConcurrentBuilds()])

properties([
    parameters([
	string(
            name: 'myvar1',
	    description: 'Example of parameter',
	    defaultValue: 'Ooooo'
		  )
		])
	])

for (p in MYLIST) {
    if (var1==p.key) {
        var2=p.value
	}
}

println var2

pipeline {
    agent { 
        label 'server'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("PREPARE") {
            steps {
                sh 'echo Cleaning workspace'
                cleanWs()
            }
        }
        stage("Second step") {
            steps {
                sh 'hostname'
            }
        }
    }
}
